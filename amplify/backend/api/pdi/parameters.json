{
  "cors": {
    "allowOrigins": [
      "*"
    ],
    "allowMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS"
    ],
    "allowHeaders": [
      "Content-Type",
      "Authorization",
      "Accept"
    ],
    "exposeHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "maxAge": 86400
  }
} 